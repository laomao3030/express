.pickup {
  margin-top: 20rpx;
}
.person-info {
  height:178rpx;
  padding: 0 30rpx;
  display: flex;
  align-items: center;
  background: rgba(255, 255, 255, 1);
}

.left {
  width: 44rpx;
  height: 44rpx;
  border: 2rpx solid rgba(255, 139, 3, 1);
  border-radius: 50%;
  font-size: 26rpx;
  line-height: 44rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  text-align: center;
  color: rgba(255, 139, 3, 1);
}

.middle {
  margin-left: 26rpx;
  flex: 1;
  font-size: 30rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(0, 0, 0, 1);
  /* border-right: 1rpx solid rgba(220, 220, 220, 1); */
}
.aline {
  width:1rpx;
  height:60rpx;
  background:rgba(220,220,220,1);
}
.middle view:nth-child(2) {
  padding: 17rpx 0 5rpx 0;
  font-size: 22rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(74, 74, 74, 1);
  text-overflow: ellipsis;
  overflow : hidden;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.right {
  padding-left: 46rpx;
  font-size: 26rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(0, 140, 240, 1);
}

.write {
  padding-left: 29rpx;
  padding-top: 22rpx;
  font-size:22rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(0,0,0,1);
}

.express-info {
  margin-top: 17rpx;
  padding: 0 30rpx;
  background: #ffffff;
}

.express-info>view {
  display: flex;
  align-items: center;
  font-size: 28rpx;
  line-height: 87rpx;
  border-bottom: 1rpx solid rgba(220, 220, 220, 1);
}

.express-info>view:last-child {
  border-bottom: none;
}

.express-info input {
  flex: 1;
  line-height: normal;
}

.label {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 115rpx;
  padding-right: 47rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(74, 74, 74, 1);
  text-align:justify;
  text-justify:inter-ideograph;
}

.special {
  justify-content: space-around;
}

.range {
  flex: 1;
  display: flex;
  align-items: center;
}

.range>text {
  display: inline-block;
  margin-left: 26rpx;
  padding: 17rpx;
  font-size: 28rpx;
  line-height: 28rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(74, 74, 74, 1);
  border: 1rpx solid rgba(153, 153, 153, 1);
  border-radius: 5rpx;
}
.range>text:nth-child(1) {
  margin-left: 0;
}
.checked {
  background: rgba(0, 140, 240, 1);
  border-radius: 5rpx;
  color: rgba(255, 255, 255, 1) !important;
  border: none !important;
}

.help {
  display: flex;
  align-items: center;
  justify-content: space-around;
  width: 43rpx;
  height: 43rpx;
  border: 2rpx solid rgba(153, 153, 153, 1);
  border-radius: 50%;
  font-size: 39rpx;
  text-align: center;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
}

.coupon {
  justify-content: space-between;
}
.coupon>view:nth-child(2) {
  margin-right: -13rpx;
  color: #CCCCCC;
}
.price {
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(74, 74, 74, 1);
  border-bottom: none !important;
}

.delete {
  font-size: 30rpx;
  line-height: 70rpx !important;
  border-radius: 8rpx;
  color: #ffffff;
  text-align: center;
  background: #e42a2a;
}

.delete view {
  width: 100%;
}

.add {
  padding: 14rpx 28rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(0, 140, 240, 1);
  line-height: 67rpx;
}

.add view {
  background: rgba(255, 255, 255, 1);
  border: 1rpx solid rgba(0, 140, 240, 1);
  border-radius: 10rpx;
  color: #2a82e4;
  text-align: center;
}

.time {
  padding: 0 15rpx 0 24rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(74, 74, 74, 1);
  line-height: 90rpx;
  background: #ffffff;
}

.date {
  display: inline-block;
  color: rgba(0, 140, 240, 1);
}

.arrow {
  color: #CCCCCC;
}

.agree {
  display: flex;
  padding: 20rpx 30rpx 0rpx 30rpx;
  font-size: 22rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
  background: rgba(245, 245, 245, 1);
}

.cancel {
  display: flex;
  justify-content: space-around;
  align-items: center;
  margin-right: 14rpx;
  width: 30rpx;
  height: 30rpx;
  background: rgba(0, 140, 240, 1);
  border-radius: 15rpx;
}

.cancel image {
  width: 14rpx;
  height: 10rpx;
}

.money {
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(1, 1, 1, 1);
  text-align: center;
}

.money view {
  padding-top: 16rpx;
  display: inline-block;
  color: rgba(230, 0, 18, 1);
}

.money view:nth-child(2) {
  color: #a6a6a6;
}

.confrim {
  padding: 16rpx 30rpx 18rpx 30rpx;
}

.confrim view {
  height: 80rpx;
  font-size: 36rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  line-height: 80rpx;
  text-align: center;
  border-radius: 10rpx;
  color: rgba(255, 255, 255, 1);
  background: rgba(0, 140, 240, 1);
}

.explain {
  padding: 10rpx 0;
  display: flex;
  align-items: center;
}

.explain>view:nth-child(1) {
  width: 130rpx;
  font-size: 36rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  text-align: center;
  color: rgba(0, 0, 0, 1);
}

.explain>view:nth-child(2) {
  flex: 1;
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  text-align: left;
  color: rgba(78, 78, 78, 1);
}

.address-list {
  padding: 0 29rpx;
}

.header,
.header2 {
  font-size: 36rpx;
  line-height: 100rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(0, 0, 0, 1);
}

.header {
  border-bottom: 1rpx solid rgba(220, 220, 220, 1);
}

.header2 {
  background: rgba(245, 245, 245, 1);
}

.time-list {
  display: flex;
  background:rgba(245,245,245,1);
}

.day {
  width: 162rpx;
  font-size: 28rpx;
  line-height: 100rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(78, 78, 78, 1);
}

.list {
  padding: 0 29rpx;
  flex: 1;
  background: #ffffff;
}

.list>view,
.address-list>view {
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 28rpx;
  line-height: 100rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(78, 78, 78, 1);
}

.address-list>view {
  border-bottom: 1rpx solid rgba(220, 220, 220, 1);
}

.address-list image,
.time-list image {
  width: 29rpx;
  height: 29rpx;
  border-radius: 50%;
}
.active {
  background: #ffffff;
}