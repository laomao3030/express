<!--pages/orderDetail/index.wxml-->
<view class="detail">
  <view class="status">
    <text>等待确认</text>
  </view>
  <view class="tips">
    <text>订单将在24小时之后自动确认收货，如有疑问请及时</text>
    <text>联系客服</text>
  </view>
  <view class="confirm"><view>确认送达</view></view>
</view>
<view class="call">
  <view>阿达</view>
  <view>电话</view>
</view>
<block wx:if="{{detailType === 'get'}}">
  <view class="express">
    <view class="dot">
     <text>快递点</text>
    </view>
  </view>
  <view class="order"></view>
</block>
<block wx:else>
  <view class="express"></view>
  <view class="order"></view>
</block>
