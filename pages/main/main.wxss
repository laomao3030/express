page {
  background-color: #f0efef;
}

.main {
  margin-bottom: 96rpx;
}

.blank1 {
  height: 8rpx;
  background: #f0efef;
}

.blank2 {
  height: 24rpx;
  background: #f0efef;
}

.search {
  font-size: 30rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(0, 0, 0, 1);
  line-height: 98rpx;
  text-align: center;
  background: #ffffff;
}

.slide-image {
  width: 750rpx;
  height: 350rpx;
}

.item {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin: 30rpx 30rpx 0 30rpx;
  padding: 53rpx 28rpx 43rpx 30rpx;
  font-size: 40rpx;
  color: #383838;
  background: #ffffff;
}

image {
  width: 100%;
  height: 100%;
}

.image {
  width: 104rpx;
  height: 104rpx;
}

.arrow image {
  width: 19rpx;
  height: 33rpx;
  display: inline-block;
}

.content {
  flex: 1;
  padding-left: 20rpx;
}

.content view:nth-child(1) {
  font-size: 36rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
}

.content view:nth-child(2) {
  margin-top: 25rpx;
  font-size: 26rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
}