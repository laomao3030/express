.main {
  margin-bottom: 20rpx;
}

.blank1 {
  height: 8rpx;
  background: #f0efef;
}

.blank2 {
  height: 24rpx;
  background: #f0efef;
}

.search {
  position: fixed;
  top: 0;
  width: 750rpx;
  height: 86rpx;
  font-size: 30rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(0, 0, 0, 1);
  line-height: 86rpx;
  text-align: center;
  background: #ffffff;
  z-index: 98;
}
swiper {
  margin-top: 86rpx;
  height: 350rpx;
}
.slide-image {
  width: 750rpx;
  height: 350rpx;
}

.item {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin: 20rpx 30rpx 0 30rpx;
  padding: 49rpx 28rpx 40rpx 29rpx;
  font-size: 40rpx;
  color: #383838;
  background: #ffffff;
  border-radius: 5rpx;
}

image {
  width: 100%;
  height: 100%;
}

.image {
  width: 104rpx;
  height: 104rpx;
}

.arrow image {
  width: 33rpx;
  height: 33rpx;
  display: inline-block;
}

.content {
  flex: 1;
  padding-left: 20rpx;
}

.content view:nth-child(1) {
  font-size: 36rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
}

.content view:nth-child(2) {
  margin-top: 17rpx;
  font-size: 26rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
}